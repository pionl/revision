{
    "name": "stevebauman/revision",
    "description": "Revisions for Eloquent Models.",
    "tags": ["revision", "revisionable", "laravel", "history", "tracking"],
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "illuminate/support": "5.*",
        "illuminate/database": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "orchestra/testbench": "~3.1"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Stevebauman\\Revision\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stevebauman\\Revision\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
